{
  "name": "nodejs-express-typescript-typeorm-template",
  "version": "1.0.0",
  "description": "NodeJS Express TypeScript and Typeorm",
  "main": "src/index.ts",
  "scripts": {
    "tsc": "tsc",
    "start:dev": "nodemon",
    "start": "npm run build && node build/index.js",
    "build": "rimraf ./build && tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:sync": "npx typeorm schema:sync",
    "db:drop": "npx typeorm schema:drop",
    "migrations:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrations:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaughan189/nodejs-express-typescript-typeorm-template.git"
  },
  "author": "Vaughan Po <vaughan188@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vaughan189/nodejs-express-typescript-typeorm-template/issues"
  },
  "homepage": "https://github.com/vaughan189/nodejs-express-typescript-typeorm-template#readme",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.27",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.0",
    "typeorm": "^0.2.25",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-shopify": "^35.1.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint",
      "pre-push": "",
      "...": "..."
    }
  }
}